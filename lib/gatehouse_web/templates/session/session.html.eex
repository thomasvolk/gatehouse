<h2>Session</h2>

<p>
  <div>
    <label>Email:</label>
    <span><%= @session.principal.email %></span>
  </div>

  <div>
    <label>Token from session: </label>
    <div>
    <textarea rows="4" cols="30"><%= @session.token %></textarea>
    </div
  </div>

  <div>
    <label>Decoded token form URL: </label>
    <div>
    <textarea rows="4" cols="30"><%= @claims %></textarea>
    </div
  </div>
</p>
